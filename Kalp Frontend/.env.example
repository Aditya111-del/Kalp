# ===========================================
# KALP AI FRONTEND - ENVIRONMENT VARIABLES
# ===========================================
# Copy this file to .env and update with your actual values

# API Configuration - UPDATE WITH YOUR ACTUAL RENDER BACKEND URL
REACT_APP_API_URL=https://your-backend-app.onrender.com
REACT_APP_WEBSOCKET_URL=https://your-backend-app.onrender.com

# Development URLs (for local development)
REACT_APP_API_URL_DEV=http://localhost:5000
REACT_APP_WEBSOCKET_URL_DEV=http://localhost:5000

# App Configuration
REACT_APP_APP_NAME=Kalp AI
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Authentication Configuration
REACT_APP_JWT_EXPIRE=7d
REACT_APP_SESSION_TIMEOUT=1440

# API Configuration
REACT_APP_API_TIMEOUT=60000
REACT_APP_MAX_MESSAGE_LENGTH=2000
REACT_APP_TYPING_DELAY=1000

# Google OAuth (Optional)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX

# Error Reporting & Monitoring
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_LOG_LEVEL=error

# Performance Optimization
REACT_APP_ENABLE_SW=true
REACT_APP_LAZY_LOADING=true

# Security Headers
REACT_APP_CSP_ENABLED=true
REACT_APP_HTTPS_ONLY=true
